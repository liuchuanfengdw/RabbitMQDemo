<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dw.helloworld.dao.UserDao">

    <insert id="save" parameterType="com.dw.helloworld.entity.dobean.UserDo" keyProperty="id" useGeneratedKeys="true">
        insert into user (id,name,age) values (null,#{name},#{age})
    </insert>

    <insert id="saveBatch" parameterType="list">
        insert into user (id,name,age) values
        <foreach collection="list" item="user" separator=",">
            (null,#{user.name},#{user.age})
        </foreach>
    </insert>

    <select id="findAll" resultType="map">
        select * from user
    </select>

    <select id="findById" parameterType="long" resultType="com.dw.helloworld.entity.dobean.UserDo">
        select * from user where id = #{id}
    </select>
</mapper>